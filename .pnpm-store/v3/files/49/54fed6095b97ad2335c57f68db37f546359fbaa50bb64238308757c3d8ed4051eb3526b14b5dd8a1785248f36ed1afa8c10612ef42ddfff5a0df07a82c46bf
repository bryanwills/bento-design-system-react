export interface AtomsFnBase {
    (...args: any): string;
    properties: Set<string>;
}
declare type GetClassNameOptions = {
    className?: string;
    atomicClasses?: string;
    defaultClassName?: string;
};
export declare function getClassName({ className, atomicClasses, defaultClassName, }: GetClassNameOptions): string | undefined;
export declare function extractAtomsFromProps<AtomsFn extends AtomsFnBase>(props: any, atomsFn: AtomsFn): {
    hasAtomProps: boolean;
    atomProps: Record<string, unknown>;
    otherProps: Record<string, unknown>;
};
export {};
