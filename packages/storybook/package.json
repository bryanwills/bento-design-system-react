    "chromatic": "^6.3.4",
    "eslint": "^8.6.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-sort-export-all": "^1.2.1",
    "lint-staged": "^12.1.7",
    "lodash.orderby": "^4.6.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
